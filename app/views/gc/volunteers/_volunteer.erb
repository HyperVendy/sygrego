<tr valign="top">
	<td> 
		<%= volunteer.volunteer_type.name %></span></br>
	</td>
	
	<td> 
		<%= link_to h(volunteer.description), edit_gc_volunteer_path(volunteer) %></span></br>
	</td>

	<% if volunteer.participant %>	
		<td> 
			<%= h(volunteer.participant.name) %>
		</td>
	<% else %>
		<td>&nbsp</td>
	<% end %>

	<td> 
		<%= volunteer.session_name %></span></br>
	</td>

	<td> 
		<%= volunteer.venue_name %></span></br>
	</td>
</tr>
