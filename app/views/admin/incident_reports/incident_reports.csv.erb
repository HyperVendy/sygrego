<%- headings = %w{Section Session Venue Description Name ActionTaken OtherInfo } -%>
<%= headings.join(',') %>
<%= CSV.generate do |csv|
  @incident_reports.each do |incident_report|
  	line = [ incident_report.section,
			 incident_report.session,
			 incident_report.venue,
			 incident_report.description,
			 incident_report.name,
			 incident_report.action_taken,
			 incident_report.other_info
		   ]
    csv << line
  end
end.html_safe %>
