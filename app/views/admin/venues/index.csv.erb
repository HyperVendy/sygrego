<%- headings = %w{RowID Active Name Address } -%>
<%= headings.join(',') %>
<%= CSV.generate do |csv|
    @venues.each do |venue|
	    line =  [  
		          venue.database_code,
              venue.active,
    				  venue.name,
		          venue.address
                ]

    csv << line
  end
end.html_safe %>