<%- headings = %w{RowID Abbr Name ShortName Coming Status NewGroup   
	              LastYear Admin TradingName Address Suburb Postcode Phone 
	              Email Website Denomination } -%>
<%= headings.join(',') %>
<%= CSV.generate do |csv|
  @groups.each do |group|
  	line = [ group.database_rowid,
  		     group.abbr,
  		     group.name,
			 group.short_name,
  		     group.coming,
  		     group.status,
  		     group.new_group,
  		     group.last_year,
  		     group.admin_use,
  		     group.trading_name,
  		     group.address,
  		     group.suburb,
  		     group.postcode,
  		     group.phone_number,
  		     group.email,
  		     group.website,
			 group.denomination ]
    csv << line
  end
end.html_safe %>
